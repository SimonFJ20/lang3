
# v = 0..10
fn hyperbole(v) {
    let cond = v < 5;
    let r = 0;
    if cond {
        r = 0;
    } else {
        r = v * 2;
    }
    return r;
}

fn add(a, b) {
    loop {
        if 5 < a {
            break;
        }
        a = a + 1;
    }

    return a + b;
}

fn main() {
    let v = add(1, 2);
    return 0;
}

